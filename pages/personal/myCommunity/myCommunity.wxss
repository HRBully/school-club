/* pages/myCommunity/myCommunity.wxss */
.list{
    margin: 0 20rpx;
}
.item{
    margin: 30rpx 0;
    height: 110rpx;
    /* box-shadow: 0px 5px 5px rgba(156, 156, 156, 0.315); */
    box-shadow: 1px 1px 4px rgba(119, 119, 119, 0.445);
    display: flex;
    align-items: center;
}
.item-left{
    width: 10px;
    height: 110rpx;
    background-color: rgba(93, 103, 226, 1);
}
.item-name{
    flex: 1;
    font-size: 17px;
    padding: 0 20rpx;
}
.item-btn-icon {
    width: 12px;
    height: 24px;
    margin-right: 30rpx;
}

button{
    width: 300rpx;
    position: absolute;
    bottom: 100rpx;
    left: 225rpx;
    background-color: rgba(93, 103, 226, 1);
    color: #fff;
}